{
  "name": "omniflow",
  "version": "1.0.0",
  "author": "sanjeev kumar",
  "license": "MIT",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "prepare": "tsc",
    "build": "yarn && cd client && yarn && yarn build",
    "frontend": "cd client && yarn dev",
    "dev": "concurrently \"yarn start-dev\" \"yarn frontend\"",
    "start-dev": "ts-node-dev --files src/app.ts",
    "knex:migrate:make": "knex --knexfile src/database/knexfile.ts migrate:make -x ts",
    "knex:migrate:latest": "knex --knexfile src/database/knexfile.ts migrate:latest",
    "knex:migrate:rollback": "knex --knexfile src/database/knexfile.ts migrate:rollback"
  },
  "dependencies": {
    "@overnightjs/jwt": "^1.2.1",
    "aws-sdk": "2.701.0",
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "express-validator": "^6.14.2",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.4",
    "mongodb": "^4.10.0",
    "mongoose": "^6.6.1",
    "morgan": "^1.10.0",
    "multer": "1.4.2",
    "multer-s3": "2.9.0",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@overnightjs/core": "^1.7.6",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-jwt": "^6.0.4",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^3.0.0",
    "@types/node": "^18.7.8",
    "concurrently": "^7.4.0",
    "dotenv": "^16.0.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  }
}
